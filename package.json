{
  "name": "myceph-server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel ./src -d ./build/src --copy-files",
    "build": "npm run clean && npm run build-babel",
    "migrate": "cd src && sequelize-cli db:migrate",
    "migrate:undo": "cd src && sequelize-cli db:migrate:undo:all",
    "production": "node ./build/src/app.js",
    "dev": "nodemon --exec ./node_modules/.bin/babel-node ./src/app.js"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/runtime": "^7.20.13",
    "@pm2/io": "^5.0.0",
    "axios": "^1.3.2",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "chalk": "^2.4.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.17.3",
    "fs": "^0.0.1-security",
    "helmet": "^6.0.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "mysql2": "^3.1.0",
    "node-rsa": "^1.1.1",
    "nodemailer": "^6.9.1",
    "nodemon": "^2.0.20",
    "passport": "^0.6.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-local": "^1.0.0",
    "pg": "8.7.1",
    "pg-hstore": "2.3.4",
    "pug": "2.0.0-beta11",
    "redis": "^4.6.5",
    "sequelize": "^6.30.0",
    "socket.io": "^4.6.1",
    "tough-cookie": "^4.1.2",
    "useragent": "^2.3.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "sequelize-cli": "^6.6.0"
  }
}
